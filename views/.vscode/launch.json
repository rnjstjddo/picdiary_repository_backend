{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "sourceMap": true,
      "trace": true
    },
    {
      "name": "Node",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev"
    }
  ],
  "compounds": [
    {
      "name": "Debug",
      "configurations": ["Chrome", "Node"]
    }
  ]
}
